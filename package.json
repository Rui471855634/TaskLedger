{
  "name": "taskledger",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm_config_cache=.npm-cache npm run dev -w apps/web",
    "build": "npm_config_cache=.npm-cache npm run build -w apps/web",
    "preview": "npm_config_cache=.npm-cache npm run preview -w apps/web",
    "lint": "npm_config_cache=.npm-cache npm run lint -w apps/web"
  }
}

